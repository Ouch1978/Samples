<Application x:Class="Wpf_ObjectDataSource.App" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" StartupUri="MainWindow.xaml"
		xmlns:local="clr-namespace:Wpf_ObjectDataSource"
		xmlns:properties="clr-namespace:Wpf_ObjectDataSource.Properties">
	<Application.Resources>
		<ResourceDictionary>
			<ObjectDataProvider x:Key="Resources" ObjectType="{x:Type local:CulturesHelper}"
					MethodName="GetResourceInstance"></ObjectDataProvider>
			<ObjectDataProvider x:Key="CultureResourcesDS" ObjectType="{x:Type local:CulturesHelper}" />
		</ResourceDictionary>
	</Application.Resources>
</Application>
